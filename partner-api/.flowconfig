[ignore]
<PROJECT_ROOT>/flow-typed/npm/.*
<PROJECT_ROOT>/lib/.*
<PROJECT_ROOT>/docs/.*
<PROJECT_ROOT>/test/compiled/.*

[include]

[libs]

[options]
emoji=true
module.name_mapper='^~\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^@\/\(.*\)$' -> '<PROJECT_ROOT>/test/src/\1'

suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
suppress_type=$FlowIgnore
suppress_type=$FlowFixMe
